        <activity android:name="com.org.ardemo.MainActivity"
            android:launchMode="singleTop">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        -->
        <activity android:name="com.org.ardemo.MainActivity" />
        <activity android:name="com.org.ardemo.GalleryActivity" />
        <activity android:name="com.org.ardemo.ViewImageActivity" />
        <activity android:name="com.org.ardemo.SearchActivity" android:launchMode="singleTop">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.org.ardemo.ViewProductActivity" />
